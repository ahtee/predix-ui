{
  "source": "./src",
  "destination": "./esdocs",
  "plugins": [
  {
    "name": "esdoc-standard-plugin",
    "option": {
      "brand": {
        "title": "px-components-react",
        "author": "Jonnie Spratley"
      },
      "manual": {
        "index": "./docs/getting-started.md",
        "asset": "./docs/assets",
        "files": [
          "./docs/overview.md",
          "./docs/installation.md",
          "./docs/usage.md",
          "./docs/tutorial.md",
          "./docs/configuration.md",
          "./docs/example.md",
          "./CHANGELOG"
        ]
      }
    }
  }]
}
