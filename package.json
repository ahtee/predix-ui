{
  "name": "px-components-react",
  "description": "px-* web components in React.js components",
  "author": "Jonnie Spratley",
  "user": "jonniespratley",
  "version": "0.0.1",
  "scripts": {
    "plop": "./node_modules/.bin/plop",
    "start": "webpack-dev-server --env dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --cache",
    "gh-pages": "webpack --env ghPages",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --env ghPages --profile --json > stats.json",
    "dist": "webpack --env dist",
    "dist:es6": "rimraf ./dist/es6 && BABEL_ENV=es6 babel ./src --out-dir ./dist/es6",
    "dist:min": "webpack --env distMin",
    "dist:modules": "rimraf ./dist/modules && BABEL_ENV=modules babel ./src --out-dir ./dist/modules",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am ''",
    "prepublish": "npm run dist:es6 && npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist/px.js",
  "module": "dist/px.js",
  "jsnext:main": "dist/px.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "catalog": "^2.5.3",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.1",
    "eslint": "^4.7.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "git-prepush-hook": "^1.0.2",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "imports-loader": "^0.7.1",
    "jest": "^21.1.0",
    "json-loader": "^0.5.4",
    "plop": "^1.8.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "purecss": "^1.0.0",
    "px-theme": "https://github.com/PredixDev/px-theme.git",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-github-corner": "^1.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonniespratley/px-components-react.git"
  },
  "homepage": "https://jonniespratley.github.io/px-components-react/",
  "bugs": {
    "url": "https://github.com/jonniespratley/px-components-react/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePathIgnorePatterns": [
      "bower_components",
      "plop-templates",
      "docs"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "node-sass": "^4.5.3",
    "px-table-view": "https://github.com/PredixDev/px-table-view.git",
    "sass-loader": "^6.0.6",
    "strip-json-comments": "^2.0.1",
    "styled-jsx": "^1.0.10"
  }
}
